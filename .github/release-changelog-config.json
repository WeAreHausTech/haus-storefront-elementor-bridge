{
    "categories": [
        {
            "title": "Features",
            "labels": [
                "feat",
                "feature"
            ]
        },
        {
            "title": "Fixes",
            "labels": [
                "fix",
                "bug"
            ]
        }
    ],
    "template": "# Version #{{TO_TAG}}\n\n#{{CHANGELOG}}",
    "pr_template": "- #{{TITLE}} (#{{URL}})",
    "empty_template": "# Version #{{TO_TAG}}\n\nNo features or fixes in release.",
    "fallback_to_all_commits": true,
    "ignore_commits": [
        "chore\\(release\\):",
        "refactor:",
        "docs:",
        "style:",
        "perf:",
        "test:",
        "chore:"
    ]
}