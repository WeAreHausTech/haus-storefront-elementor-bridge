{
    "categories": [
        {
            "title": "Features",
            "labels": [
                "feat",
                "feature"
            ]
        },
        {
            "title": "Fixes",
            "labels": [
                "fix",
                "bug"
            ]
        }
    ],
    "template": "## Version {{TO_TAG}}\n\n{{CHANGELOG}}\n",
    "pr_template": "- {{TITLE}} ({{URL}})",
    "fallback_to_all_commits": true,
    "ignore_commits": [
        "chore\\(release\\):",
        "refactor:",
        "docs:",
        "style:",
        "perf:",
        "test:",
        "chore:"
    ]
}